# App Configuration
APP_NAME=HamLogbook
APP_VERSION=1.0.0
APP_ENV=development

# API Keys (for premium features)
QRZ_API_KEY=your_qrz_api_key_here
QRZ_API_USERNAME=your_qrz_username_here
HAMQTH_API_KEY=your_hamqth_api_key_here

# Cloud Sync (for premium features)
CLOUD_SYNC_ENABLED=false
CLOUD_SYNC_API_URL=https://api.hamlogbook.com
CLOUD_SYNC_API_KEY=your_cloud_api_key_here

# DX Cluster
DX_CLUSTER_ENABLED=false
DX_CLUSTER_HOST=dxcluster.example.com
DX_CLUSTER_PORT=7300

# Subscription Management (RevenueCat)
REVENUECAT_API_KEY=your_revenuecat_api_key_here
REVENUECAT_APPLE_KEY=your_apple_key_here
REVENUECAT_GOOGLE_KEY=your_google_key_here

# Analytics & Monitoring
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_ENABLED=true

# Feature Flags
FEATURE_DX_CLUSTER=false
FEATURE_CLOUD_SYNC=false
FEATURE_CONTEST_MODE=true
FEATURE_HARDWARE_INTEGRATION=false
